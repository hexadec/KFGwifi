<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.kfg.wifimanager" >
    <!--android:versionCode="33"
    android:versionName="0.33" >-->

    <!--<uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />-->

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.VIBRATE" />
	<uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
		
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="hu.kfg.wifimanager.KFGreceiver"
			android:exported="true">
            <intent-filter android:priority="100" >
				<action android:name="android.net.wifi.STATE_CHANGE"/>
                <action android:name="android.net.wifi.supplicant.CONNECTION_CHANGE" />
				<action android:name="android.intent.action.USER_PRESENT" />
				<action android:name="hu.kfg.wifimanager.MANUAL_LOGIN" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="hu.kfg.wifimanager.TimeoutKiller"
            android:exported="false" >
        </receiver>
		<meta-data android:value="true" android:name="xposedmodule" android:minSdkVersion="21"/>
        <meta-data android:value="30" android:name="xposedminversion"/>
        <meta-data android:value="KFGwifi extension, sends android.net.netmon.captive_portal_logged_in after captive protal was logged in" android:name="xposeddescription"/>
    </application>
</manifest>
